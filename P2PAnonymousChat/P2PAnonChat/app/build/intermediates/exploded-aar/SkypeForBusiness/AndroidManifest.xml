<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.microsoft.office.sfb.appsdk"
    android:versionCode="1"
    android:versionName="0.8.0.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true" >
        <service
            android:name="com.microsoft.applications.telemetry.TelemetryService"
            android:enabled="true"
            android:exported="false"
            android:label="Telemetry Service"
            android:stopWithTask="false" />
        <service
            android:name="com.microsoft.office.sfb.appsdk.EnterpriseAuthenticationService"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.accounts.AccountAuthenticator" />
            </intent-filter>

            <meta-data
                android:name="android.accounts.AccountAuthenticator"
                android:resource="@xml/enterprise_authenticator" />
        </service>
    </application>

</manifest>